public class Main{
    
    public static void main(String[] args){
        Cliente cliente = new Cliente();
        Trabajador trabajador = new Trabajador();
        cliente.edad = 54;
        cliente.nombre = "Paula";
        cliente.telefono = 773648763;
        cliente.credito = 170.76;
        trabajador.edad = 45;
        trabajador.nombre = "Roberto";
        trabajador.telefono = 378624342;
        trabajador.salario = 970;

        System.out.printf("El cliente se llama: " + cliente.nombre + ". " + "La edad es: " + cliente.edad + ". " + "El telefono: " + cliente.telefono + ". " + "El credito es: " + cliente.credito + "\n");
        System.out.printf("El trabajador se llama: " + trabajador.nombre + ". " + "La edad es: " + trabajador.edad + ". " + "El telefono: " + trabajador.telefono + ". " + "El salario es: " + trabajador.salario + "\n");

    }
}
class Persona{
    int edad;
    String nombre;
    int telefono;
    
}


class Cliente extends Persona{
    double credito;
  
}

class Trabajador extends Persona{
    double salario;

}

